#include <stdio.h>
int main(){
    int n1,n2,n3;
    printf("Enter the value n1:");
    scanf("%d",&n1);
    printf("Enter the value n2:");
    scanf("%d",&n2);
    printf("Enter the value n3:");
    scanf("%d",&n3);
    if(n1>n2 && n1>n3){
        printf("%d is greater",n1);
    }
    else if(n2>n1 && n2>n3){
        printf("%d is greater",n2);
        
    }
    else{
        printf("%d is greater",n3);
    }
}
